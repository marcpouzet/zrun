(* recursive functions defined at top level *)
let rec f<<n>>(x) returns o 
  if n=0 then o = x
  else o = g<<n-1>>(x+1)

and g<<n>>(x) = g<<n-1>>(x)
  
let ff () =
    let rec f<<n>>(x) = 
      match n with | 0 -> x | n -> g<<n-1>>(x+1)
    and g<<n>>(x) = g<<n-1>>(x) in
    f<<10>>(0)


let main1 () = f<<10>>(0)
let main2 () = ff ()


